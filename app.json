{
  "expo": {
    "name": "OneBitForms",
    "slug": "onebitforms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/onebitforms-icon.png",
    "scheme": "onebitforms",
    "deepLinks": [
      "forms/public/*"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/onebitforms-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.onebitforms"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/onebitforms-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/onebitforms-logo-white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6750A4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
